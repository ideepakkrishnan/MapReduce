/**
 * 
 */
package com.neu.pdp.resources;

import java.util.List;

/**
 * Stores the execution timer readings 
 * @author ideepakkrishnan
 */
public class Result {
	private float minExecutionTime;
	private float maxExecutionTime;
	private float avgExecutionTime;
	private List<Float> executionTimes;
	
	/**
	 * @return the minExecutionTime
	 */
	public float getMinExecutionTime() {
		return minExecutionTime;
	}
	
	/**
	 * @param minExecutionTime the minExecutionTime to set
	 */
	public void setMinExecutionTime(float minExecutionTime) {
		this.minExecutionTime = minExecutionTime;
	}
	/**
	 * @return the maxExecutionTime
	 */
	public float getMaxExecutionTime() {
		return maxExecutionTime;
	}
	
	/**
	 * @param maxExecutionTime the maxExecutionTime to set
	 */
	public void setMaxExecutionTime(float maxExecutionTime) {
		this.maxExecutionTime = maxExecutionTime;
	}
	
	/**
	 * @return the avgExecutionTime
	 */
	public float getAvgExecutionTime() {
		return avgExecutionTime;
	}
	
	/**
	 * @param avgExecutionTime the avgExecutionTime to set
	 */
	public void setAvgExecutionTime(float avgExecutionTime) {
		this.avgExecutionTime = avgExecutionTime;
	}
	
	/**
	 * @return the executionTimes
	 */
	public List<Float> getExecutionTimes() {
		return executionTimes;
	}

	/**
	 * @param executionTimes the executionTimes to set
	 */
	public void setExecutionTimes(List<Float> executionTimes) {
		this.executionTimes = executionTimes;
	}
	
}
